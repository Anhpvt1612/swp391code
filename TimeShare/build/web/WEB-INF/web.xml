<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>AuthenticationController</servlet-name>
        <servlet-class>Controller.AuthenticationController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditTimeShares</servlet-name>
        <servlet-class>Controller.EditTimeShares</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteTimeShares</servlet-name>
        <servlet-class>Controller.DeleteTimeShares</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimeShareService</servlet-name>
        <servlet-class>Service.TimeShareService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuthenticationController</servlet-name>
        <url-pattern>/auth</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditTimeShares</servlet-name>
        <url-pattern>/EditTimeShares</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteTimeShares</servlet-name>
        <url-pattern>/DeleteTimeShares</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimeShareService</servlet-name>
        <url-pattern>/TimeShareService</url-pattern>
    </servlet-mapping>
</web-app>
